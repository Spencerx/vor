{
  "name": "vor-image-client",
  "version": "1.0.0",
  "description": "Light-weight client for Rasperry PI 2 to handle web camera",
  "main": "app/index.js",
  "engines": {
    "node": "5.0.0"
  },
  "scripts": {
    "start": "NODE_PATH=. $(npm bin)/nodemon --harmony_destructuring --harmony_modules --harmony_array_includes ./app/index",
    "serve": "NODE_PATH=. NODE_ENV=production node --harmony_destructuring --harmony_modules --harmony_array_includes ./app/index.js"
  },
  "keywords": [
    "Vor",
    "Iot"
  ],
  "dependencies": {
    "nodemon": "^1.8.1",
    "rx": "^4.0.7",
    "socket.io": "^1.3.7"
  }
}
